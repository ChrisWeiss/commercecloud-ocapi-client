{
    "name": "ocapi-client",
    "version": "0.1.0",
    "description": "An ES6 JavaScript Client for Salesforce Open Commerce API",
    "license": "SEE LICENSE IN LICENSE",
    "main": "src/index.js",
    "scripts": {
        "copyright:lint": "copyright 'src/**/*.js' 'bin/**/*.js' 'test/**/*.js'",
        "copyright:fix": "copyright --fix --update 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'",
        "docs": "jsdoc -c .jsdocrc",
        "lint": "npm run lint:js && npm run copyright:lint",
        "lint:js": "eslint '**/*.js'",
        "lint:fix": "npm run lint:js -- --fix",
        "test": "mocha --compilers js:babel-core/register $(find test -name '*Api.spec.js')",
        "build": "babel src --out-dir lib"
    },
    "browser": {
        "fs": false
    },
    "dependencies": {
        "atob": "^2.0.3",
        "babel": "^6.23.0",
        "babel-cli": "^6.24.1",
        "btoa": "^1.1.2",
        "express": "^4.16.2",
        "http-proxy-middleware": "^0.17.4",
        "superagent": "3.5.2"
    },
    "devDependencies": {
        "babel-core": "6.18.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "3.19.0",
        "eslint-plugin-import": "2.3.0",
        "eslint-import-resolver-webpack": "0.8.1",
        "expect.js": "~0.3.1",
        "jsdoc": "^3.4.3",
        "mobify-code-style": "2.8.1",
        "mocha": "~2.3.4",
        "sinon": "1.17.3"
    }
}
